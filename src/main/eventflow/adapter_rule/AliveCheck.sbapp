        <box disabled="true" name="Filter" type="filter">
            <input port="1" stream="out:Metronome_1"/>
            <output port="1" stream="out:Filter_1"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="to_milliseconds(time)-lastTime&gt;InputInterval"/>
        </box>
        <box name="Filter2" type="filter">
            <input port="1" stream="ReloadCommandIn"/>
            <output port="1" stream="InputIntervalConfig"/>
