                    <field name="Layer5" type="string"/>
                    <field name="Layer6" type="string"/>
                    <field name="Layer7" type="string"/>
                    <field name="Layer8" type="string"/>
                    <field name="Layer9" type="string"/>
                    <field name="Layer10" type="string"/>
